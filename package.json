{
  "name": "witch-duolingo",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@google-cloud/translate": "^5.1.4",
    "@testing-library/user-event": "^7.1.2",
    "body-parser": "^1.19.0",
    "core-js": "^3.6.4",
    "express": "^4.17.1",
    "node-sass-chokidar": "^1.4.0",
    "normalize.css": "^8.0.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-router-dom": "^5.1.2",
    "react-scripts": "5.0.1"
  },
  "scripts": {
    "start": "npm run build-css npm run watch-css && react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "./node_modules/eslint/bin/eslint.js ./src/*/*.js",
    "build-css": "node-sass-chokidar src/ -o src/ --recursive",
    "watch-css": "npm run build-css && node-sass-chokidar src/styles/sass -o src/styles/css --watch"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.4.0",
    "history": "^4.10.1"
  }
}
